{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "AutoLikeYoutube",
    "description": "Youtube",
    "author": "Augusto Savi"
    ,
    "browser_action": {

   	 "default_popup": "./index.html"
    
    },
    "content_scripts": [{
        "js": ["contentscript.js"]
        ,"matches": ["*://*/*"]
    }],
    "permissions":[
        "activeTab",
        "declarativeContent",
        "https://www.youtube.com/*"
    ]
}